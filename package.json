{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "aircraft dashboard monitor",
  "engines": {
    "node": "6.11.1"
  },
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "gulp",
    "test": "jest"
  },
  "repository": "git+https://github.com/jcastellanos003/dashboard.git",
  "author": "julianc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcastellanos003/dashboard/issues"
  },
  "homepage": "https://github.com/jcastellanos003/dashboard#readme",
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/setupJest.ts",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "moduleNameMapper": {
      "(.*)": "<rootDir>/client/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "html"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|ng-canvas-gauges|canvas-gauges)"
    ]
  },
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@angular/router": "^4.3.1",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.14",
    "ajv": "^5.2.2",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "file-loader": "^0.11.2",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^20.0.4",
    "jest-preset-angular": "^2.0.5",
    "loglevel": "^1.4.1",
    "ng-canvas-gauges": "^2.0.0",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.0",
    "rxjs": "^5.4.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "zone.js": "^0.8.14"
